                        <Font name="Calibri" size="11.0" />
                    </font>
                </RadioButton>
                <RadioButton fx:id="rbtnFemale" layoutX="142.0" layoutY="213.0" mnemonicParsing="false" onAction="#isFemale" text="Female" textFill="#fafafa">
