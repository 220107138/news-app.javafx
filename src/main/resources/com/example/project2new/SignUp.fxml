<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project2new.SignUpController">
    <children>
        <Pane layoutX="352.0" prefHeight="400.0" prefWidth="350.0" style="-fx-background-color: #008080;">
