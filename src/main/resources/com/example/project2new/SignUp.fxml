                <TextField fx:id="password" layoutX="60.0" layoutY="152.0" prefHeight="26.0" prefWidth="230.0" promptText="Password" style="-fx-border-color: #FAFAFA; -fx-background-color: transparent; -fx-border-width: 0 0 1 0;">
                    <font>
                        <Font name="Calibri" size="15.0" />
                    </font>
                </TextField>
                <Button fx:id="btnLogin" layoutX="121.0" layoutY="314.0" mnemonicParsing="false" onAction="#addData" prefHeight="35.0" prefWidth="108.0" style="-fx-border-color: #FAFAFA; -fx-background-color: transparent; -fx-border-radius: 25;" text="Sign Up" textFill="#fafafa">
